{
  "expo": {
    "name": "Assessment Factor Calculator",
    "slug": "afcalculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher_round.png",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": ["**/*"],
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.haseeb4091.AssesmentCalculation"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_round.png",
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      },
      "package": "com.haseeb4091.AssesmentCalculation",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "runtimeVersion": "49.0.15",
      "usesCleartextTraffic": true
    },
    "extra": {
      "eas": {
        "projectId": "bf62f0b1-330e-4978-9121-268834fa9083"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
